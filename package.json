{
  "name": "mayday-project",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 craco start",
    "server_client": "concurrently \"nodemon --watch server server/server.js\" \"cd client && npm start\"",
    "client": "cd client && npm start",
    "server": "nodemon --watch server server/server.js",
    "start:prod": "NODE_ENV=production PM2_HOME=/home/admin/.pm2 pm2 start server/server.js --name mayday --watch",
    "stop:prod": "PM2_HOME=/home/admin/.pm2 pm2 delete mayday || true",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then cd client && npm install && npm run build; fi",
    "build-client": "cd client && npm run build",
    "deploy": "npm run build-client && npm run start:prod",
    "lint": "eslint ./server",
    "test": "craco test",
    "build": "craco build",
    "eject": "react-scripts eject",
    "db:status": "./scripts/db-manager.sh status",
    "db:test": "./scripts/db-manager.sh test",
    "db:tables": "./scripts/db-manager.sh tables",
    "db:monitor": "./scripts/db-manager.sh monitor",
    "db:backup": "./scripts/db-manager.sh backup",
    "db:main": "./scripts/db-manager.sh main",
    "db:datatool": "./scripts/db-manager.sh datatool",
    "verify:transfer": "node scripts/verify-transfer-capability.js",
    "verify:transfer:prod": "node scripts/verify-transfer-capability.js --server http://192.168.1.14"
  },
